{
  "name": "enginaator-backend",
  "version": "1.0.0",
  "private": true,
  "description": "Backend API for Enginaator competition website",
  "main": "src/server.js",
"scripts": {
  "postinstall": "npx prisma generate",
  "deploy:migrate": "npx prisma migrate deploy",
  "db:generate": "npx prisma generate",
  "db:migrate": "npx prisma migrate dev",
  "db:reset": "npx prisma migrate reset",
  "db:studio": "npx prisma studio",
  "start": "node src/server.js",
  "dev": "nodemon src/server.js"
},

  "keywords": ["enginaator", "api", "backend"],
  "author": "Enginaator Team",
  "license": "ISC",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@prisma/client": "^6.16.3",
    "bcryptjs": "^3.0.2",
    "cloudinary": "^1.41.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "multer-storage-cloudinary": "^4.0.0",
    "zod": "^4.1.5",
    "nodemailer": "^7.0.5"
  },
  "devDependencies": {
    "jest": "^30.1.1",
    "nodemon": "^3.1.10",
    "prisma": "^6.16.3",
    "supertest": "^7.1.4"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  }
